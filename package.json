{
  "name": "fastfood-api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "build:force": "tsc --skipLibCheck --noEmitOnError",
    "start": "ts-node src/index.ts",
    "dev": "nodemon --exec ts-node --transpile-only src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -- -d src/lib/typeorm.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/lib/typeorm.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/lib/typeorm.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.2",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.2",
    "routing-controllers": "^0.11.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.22",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "@types/pg": "^8.11.13",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
